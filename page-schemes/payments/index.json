              },
              "components": {
                "buttons": [
                  {
                    "type": "script",
                    "settings": {
                      "title": "Печать нулевого чека",
                      "background": "dark",
                      "object": "atol",
                      "command": "create-operation",
                      "data": {
                        "operation_type": 0,
                        "cashbox_id": ":cashbox_id"
                      }
                    }
                  },
                  {
                    "type": "script",
                    "settings": {
                      "title": "Z1 отчёт",
                      "background": "dark",
                      "object": "atol",
                      "command": "create-operation",
                      "data": {
                        "operation_type": 1,
                        "cashbox_id": ":cashbox_id"
                      }
                    }
                  },
                  {
                    "type": "script",
                    "settings": {
                      "title": "X отчёт",
                      "background": "dark",
                      "object": "atol",
                      "command": "create-operation",
                      "data": {
                        "operation_type": 2,
                        "cashbox_id": ":cashbox_id"
                      }
                    }
                  }
                ]
              }
            }

                      "donor_property_value": "id",
                      "recipient_property": "client_id"
                    },
                    "type": "list"
                  },
                  {
                    "title": "Тип операции",
                    "type": "list",
                    "settings": {
                      "donor_object": "salePayMethods",
                      "donor_property_title": "title",
                      "donor_property_value": "article",
                      "recipient_property": "sale_type"
                    }

      "size": 4,
      "settings": [
        {
          "title": "Журнал чеков",
          "body": [
            {
              "title": "Продажи",
              "type": "header",
              "size": 4,
              "settings": {
                "description": "",
                "title": [
                  "Продажи"
                ]
              },
              "components": {
                "buttons": []
              }
            },
            {
              "title": "Приход наличными",
              "type": "analytic_widgets",
              "size": 4,
              "settings": {
                "widgets_group": "sales_statistic"
              },
              "components": {
                "filters": [
                  {
                    "title": "Дата",
                    "placeholder": "От",
                    "type": "date",
                    "settings": {
                      "size": 2,
                      "recipient_property": "start_at"
                    }
